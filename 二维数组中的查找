class Solution:
    # array 二维列表
    def Find(self, target, array):
        # write code here
        flag = 'false'
        for i in range(len(array)):
            if target in array[i]:
                flag = "true"
                break
        return flag
while True:
    try:
        solution=Solution()
        # 字符串转为list
        s=list(input())
        array=s[1]
        target=s[0]
        print(solution.Find(target, array))
    except:
        break
